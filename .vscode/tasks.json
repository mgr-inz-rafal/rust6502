{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo build"
		},
		{
			"type": "cargo",
			"command": "asm",
			"args": [
				"main::asm6502",
				"--no-color",
				"--asm-style",
				"att",
				"--target",
				"i686-unknown-linux-gnu",
				">output.asm"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo asm",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}