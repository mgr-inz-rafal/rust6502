{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo build"
		},
		{
			"type": "cargo",
			"command": "asm",
			"args": ["rust6502::asm6502", "--asm-style", "att", "--target", "i686-unknown-linux-gnu", ">output.asm"],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo asm"
		}
	]
}